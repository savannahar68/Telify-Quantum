{
   "author": "Michael Koch",
   "background": {
      "scripts": ["pref.js", "background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "19": "images/icon19_s.png",
         "38": "images/icon38_s.png"
      },
      "default_popup": "popup.html",
      "default_title": "Telify"
   },
   "content_scripts": [ {
      "css": [ "jquery/jquery-ui.css", "jquery/jquery.contextMenu.css", "contextMenuFlags.css", "message.css", "edit.css" ],
      "js": [ "jquery/jquery-3.1.0.min.js", "jquery/jquery-ui.min.js", "jquery/jquery.contextMenu.min.js", "helper.js", "jshashtable.js", "country_data.js", "pref.js", "buy.js", "inapp.js", "message.js", "util.js", "flag.js", "edit.js", "content.js" ],
      "matches": [ "\u003Call_urls>" ]
   } ],
   "description": "Converts telephone numbers into clickable links",
   "homepage_url": "https://www.ucomsrv.com/telify/about.html",
   "icons": {
      "128": "images/icon128.png",
      "16": "images/icon16_s.png",
      "48": "images/icon48.png"
   },
  "applications": {
    "gecko": {
      "id": "savannahar68@gmail.com",
      "strict_min_version": "56.0"
      }
   },
   "manifest_version": 2,
   "name": "telify",

   "options_ui": {
      "browser_style": true,
      "open_in_tab": false,
      "page": "options.html"
   },
   "permissions": [ "https://www.googleapis.com/", "https://www.google-analytics.com/", "storage", "identity", "contextMenus", "tabs", "activeTab" ],
   "short_name": "telify",
   "version": "1.0.0.5",
   "web_accessible_resources": [ "flag/*.png", "images/*.png", "jquery/images/*.png" ]
}
